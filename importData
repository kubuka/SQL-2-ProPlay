CREATE TABLE kills (
	Address VARCHAR NOT NULL, 
	Team VARCHAR NOT NULL, 
	Time DECIMAL, 
	Victim VARCHAR, 
	Killer VARCHAR, 
	Assist_1 VARCHAR, 
	Assist_2 VARCHAR, 
	Assist_3 VARCHAR, 
	Assist_4 VARCHAR, 
	x_pos VARCHAR, 
	y_pos VARCHAR
);

CREATE TABLE matchinfo (
	League VARCHAR NOT NULL, 
	Year DECIMAL NOT NULL, 
	Season VARCHAR NOT NULL, 
	Type VARCHAR NOT NULL, 
	blueTeamTag VARCHAR, 
	bResult BOOLEAN NOT NULL, 
	rResult BOOLEAN NOT NULL, 
	redTeamTag VARCHAR, 
	gamelength DECIMAL NOT NULL, 
	blueTop VARCHAR, 
	blueTopChamp VARCHAR NOT NULL, 
	blueJungle VARCHAR, 
	blueJungleChamp VARCHAR NOT NULL, 
	blueMiddle VARCHAR, 
	blueMiddleChamp VARCHAR NOT NULL, 
	blueADC VARCHAR, 
	blueADCChamp VARCHAR NOT NULL, 
	blueSupport VARCHAR, 
	blueSupportChamp VARCHAR NOT NULL, 
	redTop VARCHAR, 
	redTopChamp VARCHAR NOT NULL, 
	redJungle VARCHAR, 
	redJungleChamp VARCHAR NOT NULL, 
	redMiddle VARCHAR, 
	redMiddleChamp VARCHAR NOT NULL, 
	redADC VARCHAR, 
	redADCChamp VARCHAR NOT NULL, 
	redSupport VARCHAR, 
	redSupportChamp VARCHAR NOT NULL, 
	Address VARCHAR NOT NULL
);

CREATE TABLE monsters (
	Address VARCHAR NOT NULL, 
	Team VARCHAR NOT NULL, 
	Time DECIMAL NOT NULL, 
	Type VARCHAR NOT NULL
);

CREATE TABLE structures (
	Address VARCHAR NOT NULL, 
	Team VARCHAR NOT NULL, 
	Time DECIMAL, 
	Lane VARCHAR, 
	Type VARCHAR
);
