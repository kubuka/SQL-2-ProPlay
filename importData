CREATE TABLE LeagueofLegends (
	League VARCHAR NOT NULL, 
	Year DECIMAL NOT NULL, 
	Season VARCHAR NOT NULL, 
	Type VARCHAR NOT NULL, 
	blueTeamTag VARCHAR, 
	bResult BOOLEAN NOT NULL, 
	rResult BOOLEAN NOT NULL, 
	redTeamTag VARCHAR, 
	gamelength DECIMAL NOT NULL, 
	golddiff VARCHAR NOT NULL, 
	goldblue VARCHAR NOT NULL, 
	bKills VARCHAR NOT NULL, 
	bTowers VARCHAR NOT NULL, 
	bInhibs VARCHAR NOT NULL, 
	bDragons VARCHAR NOT NULL, 
	bBarons VARCHAR NOT NULL, 
	bHeralds VARCHAR NOT NULL, 
	goldred VARCHAR NOT NULL, 
	rKills VARCHAR NOT NULL, 
	rTowers VARCHAR NOT NULL, 
	rInhibs VARCHAR NOT NULL, 
	rDragons VARCHAR NOT NULL, 
	rBarons VARCHAR NOT NULL, 
	rHeralds VARCHAR NOT NULL, 
	blueTop VARCHAR, 
	blueTopChamp VARCHAR NOT NULL, 
	goldblueTop VARCHAR NOT NULL, 
	blueJungle VARCHAR, 
	blueJungleChamp VARCHAR NOT NULL, 
	goldblueJungle VARCHAR NOT NULL, 
	blueMiddle VARCHAR, 
	blueMiddleChamp VARCHAR NOT NULL, 
	goldblueMiddle VARCHAR NOT NULL, 
	blueADC VARCHAR, 
	blueADCChamp VARCHAR NOT NULL, 
	goldblueADC VARCHAR NOT NULL, 
	blueSupport VARCHAR, 
	blueSupportChamp VARCHAR NOT NULL, 
	goldblueSupport VARCHAR NOT NULL, 
	blueBans VARCHAR NOT NULL, 
	redTop VARCHAR, 
	redTopChamp VARCHAR NOT NULL, 
	goldredTop VARCHAR NOT NULL, 
	redJungle VARCHAR, 
	redJungleChamp VARCHAR NOT NULL, 
	goldredJungle VARCHAR NOT NULL, 
	redMiddle VARCHAR, 
	redMiddleChamp VARCHAR NOT NULL, 
	goldredMiddle VARCHAR NOT NULL, 
	redADC VARCHAR, 
	redADCChamp VARCHAR NOT NULL, 
	goldredADC VARCHAR NOT NULL, 
	redSupport VARCHAR, 
	redSupportChamp VARCHAR NOT NULL, 
	goldredSupport VARCHAR NOT NULL, 
	redBans VARCHAR NOT NULL, 
	Address VARCHAR NOT NULL
);

CREATE TABLE bans (
	Address VARCHAR NOT NULL, 
	Team VARCHAR NOT NULL, 
	ban_1 VARCHAR, 
	ban_2 VARCHAR, 
	ban_3 VARCHAR, 
	ban_4 VARCHAR, 
	ban_5 VARCHAR
);

